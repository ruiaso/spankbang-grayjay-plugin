{
  "name": "Spankbang",
  "description": "Spankbang video platform plugin for Grayjay with model search, recommendations, and improved creator profiles. Note: Authentication requires manual cookie setup.",
  "platformUrl": "https://spankbang.com",
  "author": "GreyPluginDev",
  "authorUrl": "https://github.com/GreyPluginDev",
  "sourceUrl": "https://raw.githubusercontent.com/ruiaso/spankbang-grayjay-plugin/refs/heads/main/SpankbangConfig.json",
  "repositoryUrl": "https://github.com/ruiaso/spankbang-grayjay-plugin",
  "scriptUrl": "https://raw.githubusercontent.com/ruiaso/spankbang-grayjay-plugin/refs/heads/main/SpankbangScript.js",
  "version": 2,
  "iconUrl": "https://raw.githubusercontent.com/ruiaso/spankbang-grayjay-plugin/refs/heads/main/icon.png",
  "id": "b2c3d4e5-f6a7-8901-bcde-fg2345678901",
  "scriptSignature": "",
  "scriptPublicKey": "",
  "packages": ["Http", "DOMParser", "Utilities"],
  "allowAllHttpHeaderAccess": true,
  "allowEval": true,
  "allowUrls": [
    "spankbang.com",
    ".spankbang.com",
    "www.spankbang.com",
    "sb-cd.com",
    ".sb-cd.com",
    "assets.sb-cd.com",
    "tbi.sb-cd.com",
    "hls-uranus.sb-cd.com",
    "deliver.ptgncdn.com",
    "c.ptgncdn.com",
    "thumb-p3.xhcdn.com",
    "thumb-p4.xhcdn.com",
    "thumb-p5.xhcdn.com"
  ],
  "authentication": {
    "userAgent": "Mozilla/5.0 (Linux; Android 13; Pixel 7) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Mobile Safari/537.36",
    "loginUrl": "http://localhost:5000/login",
    "completionUrl": "http://localhost:5000/login-complete",
    "cookiesToFind": ["auth", "session", "sid"],
    "allowedDomains": ["spankbang.com", "www.spankbang.com", ".spankbang.com"]
  },
  "settings": [
    {
      "variable": "authCookies",
      "name": "Authentication Cookies",
      "description": "Paste your SpankBang cookies here (format: auth=value; session=value). Click Login button for instructions on how to extract cookies from your browser.",
      "type": "string",
      "default": ""
    }
  ],
  "supportedClaimTypes": [3],
  "primaryClaimFieldType": 3,
  "changelog": {
    "1": [
      "Initial release",
      "Video playback support",
      "Search functionality",
      "Home feed",
      "Channel/Model support",
      "Related videos"
    ],
    "2": [
      "Added manual cookie-based authentication",
      "Fixed subscriptions support with proper auth",
      "Fixed playlists support with proper auth",
      "Added watch history support with thumbnails and duration",
      "Improved login instructions page",
      "Added cookie validation and expiry detection"
    ]
  }
}